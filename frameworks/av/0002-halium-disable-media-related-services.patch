From 5044cd1c197cc1a0e0d821a4cfa19b89c7fb0466 Mon Sep 17 00:00:00 2001
From: NeKit <nekit1000@gmail.com>
Date: Sun, 9 Feb 2020 18:39:54 +0100
Subject: [PATCH 02/12] (halium) disable media-related services

Their functionality is provided by minimedia in droidmedia repo instead

Change-Id: Ie7018a4d641d18330ac758126feed883efa71fec
---
 camera/cameraserver/cameraserver.rc | 2 +-
 media/audioserver/audioserver.rc    | 2 +-
 media/mediaserver/mediaserver.rc    | 4 ++--
 3 files changed, 4 insertions(+), 4 deletions(-)

diff --git a/camera/cameraserver/cameraserver.rc b/camera/cameraserver/cameraserver.rc
index e307653..864cd5b 100644
--- a/camera/cameraserver/cameraserver.rc
+++ b/camera/cameraserver/cameraserver.rc
@@ -1,4 +1,4 @@
-service cameraserver /system/bin/cameraserver
+service cameraserver /system/bin/cameraserver_HYBRIS_DISABLED
     class main
     user cameraserver
     group audio camera input drmrpc
diff --git a/media/audioserver/audioserver.rc b/media/audioserver/audioserver.rc
index 8bdb86e..dc2f462 100644
--- a/media/audioserver/audioserver.rc
+++ b/media/audioserver/audioserver.rc
@@ -1,4 +1,4 @@
-service audioserver /system/bin/audioserver
+service audioserver /system/bin/audioserver_HYBRIS_DISABLED
     class core
     user audioserver
     # media gid needed for /dev/fm (radio) and for /data/misc/media (tee)
diff --git a/media/mediaserver/mediaserver.rc b/media/mediaserver/mediaserver.rc
index 05373c9..b6f0558 100644
--- a/media/mediaserver/mediaserver.rc
+++ b/media/mediaserver/mediaserver.rc
@@ -1,9 +1,9 @@
 on property:init.svc.media=*
     setprop init.svc.mediadrm ${init.svc.media}
 
-service media /system/bin/mediaserver
+service media /system/bin/mediaserver_HYBRIS_DISABLED
     class main
     user media
     group audio camera inet net_bt net_bt_admin net_bw_acct drmrpc mediadrm
     ioprio rt 4
-    task_profiles ProcessCapacityHigh HighPerformance
+    task_profiles ProcessCapacityHigh HighPerformance
\ No newline at end of file
-- 
2.45.2

